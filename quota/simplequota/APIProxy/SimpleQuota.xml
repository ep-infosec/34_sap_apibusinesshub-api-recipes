<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<APIProxy>
    <name>SimpleQuota</name>
    <title>SimpleQuota</title>
    <description>API Proxy for simple quota, which allows 5 calls per minute</description>
    <service_code>REST</service_code>
    <life_cycle>
        <changed_by>I</changed_by>
        <created_by>I</created_by>
    </life_cycle>
    <proxyEndPoints>
        <proxyEndPoint>
            <proxyEndPointName>default</proxyEndPointName>
        </proxyEndPoint>
    </proxyEndPoints>
    <targetEndPoints>
        <targetEndPoint>default</targetEndPoint>
    </targetEndPoints>
    <policies>
        <policy type="Quota">setquota</policy>
    </policies>
    <fileResources/>
</APIProxy>
