<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ProxyEndPoint default="true">
    <name>default</name>
    <base_path>/redirect</base_path>
    <properties/>
    <routeRules>
        <routeRule>
            <name>default</name>
            <targetEndPointName>default</targetEndPointName>
            <sequence>1</sequence>
            <faultRules/>
        </routeRule>
    </routeRules>
    <faultRules/>
    <preFlow>
        <name>PreFlow</name>
        <response>
            <isRequest>false</isRequest>
            <steps>
                <step>
                    <policy_name>ExtractLocation</policy_name>
                    <condition>response.status.code=301</condition>
                    <sequence>1</sequence>
                </step>
                <step>
                    <policy_name>WriteResponse</policy_name>
                    <condition></condition>
                    <sequence>3</sequence>
                </step>
                <step>
                    <policy_name>callTarget</policy_name>
                    <condition>response.status.code=301</condition>
                    <sequence>2</sequence>
                </step>
            </steps>
        </response>
    </preFlow>
    <postFlow>
        <name>PostFlow</name>
    </postFlow>
    <conditionalFlows/>
</ProxyEndPoint>
