<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<APIProxy>
    <name>BasicAuthentication</name>
    <title>Basic Authentication</title>
    <description>API showing how the service user credential can be passed as Basic Authentication while connecting to the Target endpoint</description>
    <service_code>REST</service_code>
    <life_cycle>
        <changed_by>I</changed_by>
        <created_by>I</created_by>
    </life_cycle>
    <proxyEndPoints>
        <proxyEndPoint>
            <proxyEndPointName>default</proxyEndPointName>
        </proxyEndPoint>
    </proxyEndPoints>
    <targetEndPoints>
        <targetEndPoint>default</targetEndPoint>
    </targetEndPoints>
    <policies>
        <policy type="BasicAuth">setcredential</policy>
        <policy type="AssignMessage">assignuserdetails</policy>
    </policies>
    <fileResources/>
</APIProxy>
