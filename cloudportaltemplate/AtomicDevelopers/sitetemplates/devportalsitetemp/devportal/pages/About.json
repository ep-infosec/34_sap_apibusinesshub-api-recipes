lign: right;            display: inline-block;            width: 50%;        }        .feature_right {            text-align: left;            display: inline-block;            width: 50%;            float: right;        }        .feature_left p {            text-align: right;            padding: 0 45px 0 10px;            margin: 5px 0;        }        .feature_left p:after{            content: '';            display: block;            background: url('/sap/fiori/devportalsitetemp/devportal/images/Features/dot.png') no-repeat;            width: 15px;            height: 15px;            float: right;            margin: 8px 0 0 5px;        }        .feature_left p:before{            content: '';            display: none;            background: url('/sap/fiori/devportalsitetemp/devportal/images/Features/dot.png') no-repeat;            width: 15px;            height: 15px;            margin: 8px 5px 0 0;        }        .feature_right p {            text-align: left;            padding: 0 10px 0 45px;            margin: 5px 0;        }        .feature_right p:before{            content: '';            display: block;            background: url('/sap/fiori/devportalsitetemp/devportal/images/Features/dot.png') no-repeat;            width: 15px;            height: 15px;            float: left;            margin: 8px 0 0 5px;        }        .feature_header {            color:#007cc0;            font-weight:bold ;            margin-bottom: 10px;        }        .feature_left .feature_header {            text-align: right;            padding: 0 55px 0 0;        }        .feature_right .feature_header {            text-align: left;            padding: 0 0 0 55px;        }        @media only screen and (max-width:1240px) {            .feature_header {                font-size: 16px;            }            .feature_left p, .feature_right p {                font-size: 14px;            }        }         /*******SMARTPHONE********/        .sapUiMedia-Std-Phone .features_mobile {